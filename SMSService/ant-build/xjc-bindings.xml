<jxb:bindings version="1.0"
	xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc" 
	schemaLocation="../src/xsd/SMSService.xsd" node="/xs:schema">
	<jxb:globalBindings>
        <xjc:serializable uid="1" />
	</jxb:globalBindings>
	
  <jxb:bindings node="//xs:element[@name='attendanceMap']">
  	 <jxb:property>
    	<jxb:baseType name="com.ish.sms.service.dto.AttendanceMap&lt;String,Boolean&gt;" />
   	 </jxb:property>
  </jxb:bindings>
  <jxb:bindings node="//xs:element[@name='gradeDetailsMap']">
  	 <jxb:property>
    	<jxb:baseType name="com.ish.sms.service.dto.GradeDetailsMap&lt;com.ish.sms.service.dto.ClassSubjectReferenceDataDTO,com.ish.sms.service.dto.GradeDetailsDTO&gt;" />
   	 </jxb:property>
  </jxb:bindings>  	
  <jxb:bindings node="//xs:element[@name='reportCardDetailsMap']">
  	 <jxb:property>
    	<jxb:baseType name="com.ish.sms.service.dto.ReportCardDetailsMap&lt;com.ish.sms.service.dto.ClassExamReferenceDataDTO,com.ish.sms.service.dto.ReportCardDetailsDTO&gt;" />
   	 </jxb:property>
  </jxb:bindings>  	
</jxb:bindings>